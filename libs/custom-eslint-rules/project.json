{
  "name": "custom-eslint-rules",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "lib/custom-eslint-rules/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/lib/custom-eslint-rules",
        "main": "lib/custom-eslint-rules/src/index.js",
        "tsConfig": "lib/custom-eslint-rules/tsconfig.lib.json",
        "assets": ["lib/custom-eslint-rules/*.md"]
      }
    }
  }
}
